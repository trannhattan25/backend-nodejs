<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Update</title>
    <link rel="stylesheet" href="/css/style.css">
</head>

<body>
    <%= JSON.stringify(userEdit) %>

        <div class="topnav">
            <a href="/">Home</a>
            <a href="/create-user">Create User</a>
            <a href="#contact">Contact</a>
            <a href="#about">About</a>
        </div>
        <div class="form-add-new">
            <form action="/update" method="post">
                <fieldset>
                    <legend>Update a user</legend>
                    <div class="input-group d-none">
                        <label for="">Id:</label>
                        <input value="<%= userEdit.id %>" id="emailId" name="id" type="text">
                    </div>
                    <div class="input-group">
                        <label for="">Email:</label>
                        <input value="<%= userEdit.email %>" id="emailId" name="email" type="text">
                    </div>
                    <div class="input-group">
                        <label for="">Name:</label>
                        <input value="<%= userEdit.name %>" name="name" type="text">
                    </div>
                    <div class="input-group">
                        <label for="">City:</label>
                        <input value="<%= userEdit.city %>" name="city" type="text">
                    </div>
                    <div>
                        <button class="btn">Save</button>
                    </div>
                </fieldset>
            </form>
        </div>
</body>

</html>